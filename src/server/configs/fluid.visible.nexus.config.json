{
    "type": "fluid.visible.nexus.config",
    "options": {
        "components": {
            "server": {
                "type": "kettle.server",
                "options": {
                    "components": {
                        "authoring": {
                            "type": "fluid.authoring.nexus.app"
                        }
                    }
                }
            }
